@using Xomorod.Helper.Resources
@using Xomorod.Helper

@model TreeNode<Xomorod.Helper.Sitemap.SitemapNode>

@section head{
    <meta name=robots content=noodp>
    <meta name=robots content=noydir>
    <link rel="canonical" href="@Localization.XomorodUrl/sitemap.html" />
}



@section scripts
{
    @Styles.Render("~/Content/easyTree")
    @Scripts.Render("~/bundles/easyTree")
    <script>
        (function ($) {
            function init() {
                $('.easy-tree').EasyTree();
            }

            window.onload = init();
        })(jQuery)
    </script>
}

<div class="sitemap">
    <article role="main">

        <h1 style="padding: 50px;">
            <img src='data:image/png;base64,' alt='xomorod.com sitemap' class='logo' width='45' height='45'/>
            @Localization.Sitemap
        </h1>

        @Html.TreeViewItem(Model)

    </article>
</div>

